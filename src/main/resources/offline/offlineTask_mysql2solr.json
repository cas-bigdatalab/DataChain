
{
  "Task":{
    "name":"offlineTask_mysql2mongo",
    "taskType":"offline",
    "sql":"insert into table user select id, name, age from mysql_user",

    "srcTable":{
      "table1":{
        "driver":"mysql",
        "db":"spark",
        "table":"mysql_user",
        "columns":{"id":"Int","name":"String","age":"Int"}
      }

    },

    "mappingSpec": {
      "type": "morphlinesMapping",
      "dimensionsMap":  {
        "dimensions" : ["year:int","make:string","model:string","comment:string","blank:float"]
      }
    },

    "destTable":{

      "table1":{
        "driver":"solr",
        "table":"user",
        "columns":{"id":"Int","name":"String","age":"Int"}
      }

    }
  }
}